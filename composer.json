{
    "name": "dazzlesoftware/di",
    "type": "library",
    "description": "Dazzle DI Package",
    "keywords": ["dazzle", "framework", "dependency injection", "di", "ioc", "container"],
    "homepage": "https://github.com/dazzle-framework/di",
    "license": "GPL-3.0-or-later",
    "require": {
       "php": "^7.3.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "dazzlesoftware/dazzle-coding-standard": "^1.0|^2.0",
        "dazzlesoftware/test": "^1.0|^2.0",
        "phpunit/phpunit": "^9.0"
    },
    "provide": {
        "psr/container-implementation": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\DI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dazzle\\DI\\UnitTests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-develop": "2.x-dev"
        }
    }
}
